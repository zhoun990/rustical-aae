{
	"build": {
		"beforeDevCommand": "yarn typeshare && yarn vite",
		"beforeBuildCommand": "yarn typeshare && yarn build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "rustical-aae",
		"version": "0.0.0"
	},
	"tauri": {
		"allowlist": {
			"all": false,
			"shell": {
				"all": false,
				"open": true
			},
			"fs": {
				"all": false,
				"copyFile": false,
				"createDir": false,
				"exists": false,
				"readDir": false,
				"readFile": true,
				"removeDir": false,
				"removeFile": false,
				"renameFile": false,
				"scope": [],
				"writeFile": false
			},
			"dialog": {
				"all": false,
				"ask": false,
				"confirm": true,
				"message": false,
				"open": false,
				"save": false
			},
			"process": {
				"all": false,
				"exit": true,
				"relaunch": true,
				"relaunchDangerousAllowSymlinkMacos": false
			}
		},
		"bundle": {
			"active": true,
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "net.webbel.aae",
			"targets": "all"
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "AAE",
				"width": 1720,
				"height": 880,
				"x": -1820,
				"y": 100
			}
		]
	}
}
